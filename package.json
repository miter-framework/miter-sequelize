        "@types/sinon-chai": "^2.7.27",
        "chai": "^3.5.0",
        "chai-as-promised": "^6.0.0",
        "express": "^4.14.0",
        "jsonwebtoken": "^7.4.1",
        "miter": "^0.5.4",
        "mocha": "^3.2.0",
        "mocha-typescript": "^1.0.23",
        "sinon": "^2.1.0",
        "sinon-chai": "^2.10.0",
        "ts-node": "^3.0.2",
        "tslint": "^4.5.1",
        "typescript": "2.6.1"
