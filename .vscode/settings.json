{
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.renderWhitespace": "all",
    "editor.trimAutoWhitespace": false,
    "files.insertFinalNewline": true,
    "vsicons.presets.angular": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/npm-debug.log*": true
    },
    "typescript.tsdk": "node_modules/typescript/lib"
}
